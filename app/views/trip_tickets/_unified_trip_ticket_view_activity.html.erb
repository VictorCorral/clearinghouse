<li>
  <% if activity.respond_to?(:auditable_id) %>
    <%= formatted_activity_line(activity) %>
  <% else %>
    <%= link_to formatted_activity_line(activity), activity.is_a?(TripResult) ? trip_ticket_path(activity.trip_ticket) + '/trip_result' : [activity.trip_ticket, activity], data: {:"activity-type" => activity.class.name.underscore} %>
  <% end %>
</li>